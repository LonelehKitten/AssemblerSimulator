{
  "name": "ASMR",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@electron/remote": "^1.2.1",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@mui/material": "^5.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "bindings": "^1.5.0",
    "electron-is-dev": "^2.0.0",
    "electron-packager": "^15.4.0",
    "nan": "^2.15.0",
    "react": "^17.0.2",
    "react-ace": "^9.4.3",
    "react-dom": "^17.0.2",
    "react-electron": "0.0.0",
    "react-scripts": "4.0.3",
    "web-vitals": "^1.1.2"
  },
  "gypfile": true,
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:test": "react-scripts test",
    "react:eject": "react-scripts eject",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none npm react:start\" \"npm run electron:start\"",
    "electron:build": "npm react:build && electron-builder -c.extraMetadata.main=build/main.js",
    "electron:start": "wait-on tcp:3000 && electron .",
    "electron:deploy": "electron-builder --win --publish always -c.extraMetadata.main=build/electron.js --config=electron-builder.yml",
    "dev": "npm run electron:serve",
    "deploy": "npm run react:build && npm run electron:deploy",
    "c++:build": "electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LonelehKitten/AssemblerSimulator"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron": "^13.2.3",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^3.2.3",
    "prettier": "^2.3.2",
    "wait-on": "^6.0.0"
  }
}
